    <None Remove="Content\Levels\level7.txt" />
    <None Remove="Content\Levels\level8.txt" />
    <None Remove="Content\Levels\level9.txt" />
    <None Remove="Content\Levels\levels_status.txt" />
    <None Remove="Icon.ico" />
    <None Remove="Icon.bmp" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="Content\Levels\level11.txt">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Content\Levels\level1.txt">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
